{
  "name": "village-people",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^10.13.0"
  },
  "scripts": {
    "setup": "rm yarn.lock && yarn && node scripts/setup.js",
    "build": "yarn clean && webpack --config ./config/webpack.config.production.js",
    "build:development": "webpack-dev-server --config ./config/webpack.config.development.js",
    "clean": "rm -rf build/*",
    "lint": "eslint src/ --fix --max-warnings=0",
    "start": "yarn && yarn build:development",
    "start-dev": "yarn start & NODE_ENV='development' nodemon server",
    "server": "node server",
    "test": "yarn && node scripts/test-suite.js",
    "test:unit": "jest",
    "test:integration": "cypress run",
    "seed:test": "echo 'No script specified, seed skipped'",
    "generate-bundle-stats": "yarn clean && webpack --config config/webpack.config.production.js --profile --json > .webpack-stats.json",
    "bundle-analyzer-server": "webpack-bundle-analyzer .webpack-stats.json build/",
    "analyze-bundle": "yarn && yarn generate-bundle-stats && yarn bundle-analyzer-server",
    "format": "prettier --write \"src/**/*.+(js|jsx|json)\""
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@launchpadlab/babel-preset": "^2.0.0",
    "@launchpadlab/lp-components": "^3.19.0",
    "@launchpadlab/lp-form": "^2.2.1",
    "@launchpadlab/lp-hoc": "^5.0.6",
    "@launchpadlab/lp-redux-api": "^6.2.0",
    "@launchpadlab/lp-redux-utils": "^1.1.2",
    "@launchpadlab/lp-requests": "^4.1.0",
    "@sentry/browser": "^5.5.0",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.0.5",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.3",
    "connected-react-router": "^6.8.0",
    "copy-webpack-plugin": "^5.0.3",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "dot-index-webpack-plugin": "^5.0.0",
    "expose-env-middleware": "^2.0.0",
    "express": "^4.17.3",
    "express-enforces-ssl": "^1.1.0",
    "figaro-js": "^3.0.2",
    "file-loader": "^6.0.0",
    "googleapis": "^48.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.6",
    "http-proxy-middleware": "^1.0.3",
    "json-loader": "0.5.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.0",
    "prop-types-exact": "^1.1.2",
    "query-string": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-uid": "^2.2.0",
    "recompose": "^0.27.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.0.3",
    "redux-flash": "^2.0.0",
    "redux-form": "^8.3.1",
    "redux-modal": "^4.0.0",
    "redux-sessions": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.0.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.36.0",
    "webpack-cli": "^3.0.8",
    "xmp-escape-loader": "^2.0.0"
  },
  "devDependencies": {
    "@launchpadlab/eslint-config": "^2.4.1",
    "@launchpadlab/lp-subsection-generator": "^6.0.2",
    "@launchpadlab/prettier-config": "^1.0.0",
    "chalk": "^3.0.0",
    "cli-prompt": "^0.6.0",
    "cypress": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.3",
    "prettier": "^1.18.2",
    "react-dev-utils": "^5.0.1",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint --fix --max-warnings=0",
    "src/**/*.{js,jsx,json}": "prettier --write"
  }
}
